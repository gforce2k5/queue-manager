<div class="form-check form-check">
  <input class="form-check-input" type="radio" name="<%= field.name.kebab %>" id="<%= field.name.kebab %>-true" value="true"<%= field.value == 'true' ? ' checked' : '' %>>
  <label class="form-check-label" for="<%= field.name.kebab %>-true">Enable</label>
</div>
<div class="form-check form-check">
  <input class="form-check-input" type="radio" name="<%= field.name.kebab %>" id="<%= field.name.kebab %>-false" value="false"<%= field.value == 'false' ? ' checked' : '' %>>
  <label class="form-check-label" for="<%= field.name.kebab %>-false">Disable</label>
</div>